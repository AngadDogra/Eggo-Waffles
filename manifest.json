{
    "name" : "Argod's & AJs Extension :)", 
    "description": "Pomodoro + Dictionary Extension",
    "version": "1.1",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "index.html",
        "default_icon" : "assets/logo-removebg.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["activeTab"]
}